            break;
        default:
            raise("invalid operator: " + op);
        }
        return OpCode.NewBytes(Util.GetBytes3(0x0f, b, (byte)(0xc0 + (((int)op2) << 3) + op1)));
    }

    static function FromNameB(op : string, op1 : Mm, op2 : byte) : OpCode
    {
        byte b1 = 0, b2 = 0;
        switch (op)
        {
        case "psllw":
        case "psrlw":
        case "psraw":
            b1 = 0x71;
            break;
        case "pslld":
        case "psrld":
        case "psrad":
            b1 = 0x72;
            break;
        case "psllq":
        case "psrlq":
            b1 = 0x73;
            break;
        }
        switch (op)
        {
        case "psllw":
        case "pslld":
        case "psllq":
            b2 = 0xf0;
            break;
        case "psrlw":
        case "psrld":
        case "psrlq":
            b2 = 0xd0;
            break;
        case "psraw":
        case "psrad":
            b2 = 0xe0;
            break;
        }
        if (b1 == 0 || b2 == 0)
            raise("invalid operator: " + op);
        return OpCode.NewB(Util.GetBytes3(0x0f, b1, (byte)(b2 + op1)), op2);
    }
