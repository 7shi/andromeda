    static function GetCode(op : string) : byte
    {
        switch (op)
        {
        case "movq":
            return 0x6f;
        case "paddb":
            return 0xfc;
        case "paddw":
            return 0xfd;
        case "paddd":
            return 0xfe;
        case "paddq":
            return 0xd4;
        case "psubb":
            return 0xf8;
        case "psubw":
            return 0xf9;
        case "psubd":
            return 0xfa;
        case "psubq":
            return 0xfb;
        case "pmulhw":
            return 0xe5;
        case "pmullw":
            return 0xd5;
        case "psllw":
            return 0xf1;
        case "pslld":
            return 0xf2;
        case "psllq":
            return 0xf3;
        case "psrlw":
            return 0xd1;
        case "psrld":
            return 0xd2;
        case "psrlq":
            return 0xd3;
        case "psraw":
            return 0xe1;
        case "psrad":
            return 0xe2;
        case "punpckhbw":
            return 0x68;
        case "punpckhwd":
            return 0x69;
        case "punpckhdq":
            return 0x6a;
        case "punpcklbw":
            return 0x60;
        case "punpcklwd":
            return 0x61;
        case "punpckldq":
            return 0x62;
        case "packsswb":
            return 0x63;
        case "packssdw":
            return 0x6b;
        case "packuswb":
            return 0x67;
        default:
            raise("invalid operator: " + op);
        }
    }

    static function FromName(op : string, op1 : Mm, op2 : Mm) : OpCode
    {
        var b = GetCode(op);
        return OpCode.NewBytes(Util.GetBytes3(0x0f, b, (byte)(0xc0 + (((int)op1) << 3) + op2)));
    }

    static function FromNameA(op : string, op1 : Mm, op2 : Addr32) : OpCode
    {
        byte b;
        switch (op)
        {
        case "movd":
            b = 0x6e;
            break;
        default:
            b = GetCode(op);
            break;
        }
        return OpCode.NewA(Util.GetBytes2(0x0f, b), Addr32.NewAdM(op2, (byte)op1));
    }

    static function FromNameAM(op : string, op1 : Addr32, op2 : Mm) : OpCode
    {
        byte b;
        switch (op)
        {
        case "movd":
            b = 0x7e;
            break;
        case "movq":
            b = 0x7f;
            break;
        default:
            raise("invalid operator: " + op);
        }
        return OpCode.NewA(Util.GetBytes2(0x0f, b), Addr32.NewAdM(op1, (byte)op2));
    }

    static function FromNameD(op : string, op1 : Mm, op2 : Reg32) : OpCode
