class SectionBase
{
    var Header : SectionHeader;

    virtual get_Name : string { return null; }
    virtual Write(block : Block32) {}

    var block : Block32;
    function get_Block : Block32 { return block; }

    function ToBlock(vaddr : uint) : Block32
    {
        var ret = Block32.New(vaddr);
        Write(ret);
        block = ret;
        return ret;
    }
}
