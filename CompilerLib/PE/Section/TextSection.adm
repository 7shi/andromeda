class TextSection : SectionBase
{
    override get_Name { return ".text"; }

    var OpCodes : OpCode[];

    override Write(block : Block32)
    {
        for (int i = 0; i < OpCodes.Length; i++)
        {
            var op = OpCodes[i];
            op.Address.Value = block.Current;
            op.Write(block);
        }
    }
}
