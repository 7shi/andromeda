class TextSection : SectionBase
{
    function get_Name : string
    {
        return ".text";
    }

    var OpCodes : OpCode[];

    override Write(block : Block)
    {
        for (int i = 0; i < OpCodes.Length; i++)
        {
            var op = OpCodes[i];
            op.Address.Value = block.Current;
            op.Write(block);
        }
    }
}
